package pages

templ EmployeesPrint(id int) {
	<div class="print-layout">
		<div class="print-sidebar">sidebar</div>
		<div class="print-content">
			<div class="print-content-item">content { id }</div>
		</div>
	</div>
	<style>
    .print-layout {
      display: flex;
      height: 100vh;
      & > .print-sidebar {
        width: 250px;
        background-color: var(--sidebar-bg);
      }
      & > .print-content {
        flex: 1;
      }
    }
  </style>
}
